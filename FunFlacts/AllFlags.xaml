<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FunFlacts.AllFlags"
             Title="Fun with Flags">
    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="EditToolbar" Clicked="EditButton_OnClicked">
        </ToolbarItem>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>

            <ListView x:Name="FlagListView" ItemTapped="FlagListView_OnItemTapped">
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>